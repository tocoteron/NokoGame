<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GameStartController">
	<children>
		<AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
			<children>
				<ImageView fitHeight="407.0" fitWidth="670.0" layoutY="-1.0">
					<image>
						<Image url="@images/yama_fukei.png" />
					</image>
				</ImageView>
				<Button fx:id="GameStart1" layoutX="228.0" layoutY="210.0" mnemonicParsing="false" onAction="#GameStart1ButtonAction" prefHeight="48.0" prefWidth="200.0" text="１人でPLAY" />
				<Button fx:id="GameStart2" layoutX="228.0" layoutY="264.0" mnemonicParsing="false" onAction="#GameStart2ButtonAction" prefHeight="48.0" prefWidth="200.0" text="２人でPLAY" />
				<ComboBox fx:id="SelectMapsize" editable="true" layoutX="228.0" layoutY="332.0" prefWidth="200.0" promptText="マス目">
					 <items>
						<FXCollections fx:factory="observableArrayList">
							<String fx:value="4 x 4"/>
							<String fx:value="6 x 6"/>
							<String fx:value="8 x 8"/>
							<String fx:value="10 x 10"/>
							<String fx:value="12 x 12"/>
						</FXCollections>
					</items>
				</ComboBox>
				<ImageView fitHeight="150.0" fitWidth="200.0" layoutX="453.0" layoutY="183.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@images/takenoko.png" />
					</image>
				</ImageView>
				<ImageView fitHeight="150.0" fitWidth="200.0" layoutX="59.0" layoutY="191.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@images/kinoko.png" />
					</image>
				</ImageView>
				<ImageView fitHeight="111.0" fitWidth="287.0" layoutX="177.0" layoutY="120.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@images/oseroman.png" />
					</image>
				</ImageView>
			</children>
		</AnchorPane>
	</children>
</VBox>

